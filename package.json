{
  "name": "webcrypt",
  "version": "1.0.0",
  "description": "A WebAssembly port of OpenSSL.",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:openssl",
    "build:openssl": "node scripts/build-openssl.mjs",
    "build:demo": "vite build --config vite.demo.config.ts",
    "dev": "vite dev",
    "lint": "biome lint ./src ./demo ./scripts ./native vite.config.ts vite.demo.config.ts",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run",
    "test": "npm run check",
    "check": "npm run lint && npm run typecheck && npm run test:unit",
    "validate": "npm run check"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/node": "^24.5.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
